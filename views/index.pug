extends layout

block content
  h1= title
  p Welcome to Navya Sri Alaparthi #{title} 
  button(type="button", class="btn btn-info", onclick="update()") Press Me
  
  div(id ="target") text of Display
  script.  
   var client_count=0;
     function update() {
       client_count++;
       document.getElementById("target").innerHTML=`Value now ${client_count}`;
     }